{
  "Mocks single function from module": {
    "prefix": "mmock",
    "body": [
      "jest.mock('$1', () => {",
      "  return $0",
      "});"
    ],
    "description": "Mocks a module in jest"
  },
  "Mocks a module using a generic implementation": {
    "prefix": "smock",
    "body": [
      "jest.mock('$1');$0"
    ],
    "description": "Mocks a module using generic implementation from __mocks__ folder"
  }
}